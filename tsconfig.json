{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2015",
    "module": "CommonJS",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "assumeChangesOnlyAffectDirectDependencies": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true
  }
}
